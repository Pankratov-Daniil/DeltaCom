<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="com.deltacom.app.entity.Client" table="`client`">

        <id name="id" column="idClient">
            <generator class="increment"/>
        </id>

        <property name="firstName" column="firstNameClient"/>
        <property name="lastName" column="lastNameClient"/>
        <property name="birthDate" column="birthDateClient"/>
        <property name="passport" column="passportClient"/>
        <property name="address" column="addressClient"/>

        <property name="email" column="emailClient"/>
        <property name="password" column="passwordClient"/>
    </class>
</hibernate-mapping>
