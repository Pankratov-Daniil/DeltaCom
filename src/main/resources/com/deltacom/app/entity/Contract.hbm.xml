<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="com.deltacom.app.entity.Contract" table="`contract`">

        <id name="number" column="numberContract">
            <generator class="assigned"/>
        </id>

        <property name="idTariff" column="idTariff"/>

        <set name="options" table="contract_option">
            <key column="numberContract"/>
            <many-to-many column="idOption" class="com.deltacom.app.entity.Option"/>
        </set>
    </class>

</hibernate-mapping>
